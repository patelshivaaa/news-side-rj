{
  "expo": {
    "name": "News App",
    "slug": "newsapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/news.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/news.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
